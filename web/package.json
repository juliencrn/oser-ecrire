{
  "name": "oser-ecrire-web",
  "private": true,
  "description": "A simple starter using Typescript in front & back side with eslint & prettier",
  "version": "0.1.0",
  "author": "Julien CARON <juliencaron@protonmail.com>",
  "keywords": [
    "gatsby",
    "typescript",
    "eslint",
    "prettier"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run build:app && npm run build:lambda",
    "build:app": "npm run clean && gatsby build",
    "build:lambda": "netlify-lambda build functions-src",
    "dev": "npm run start",
    "develop": "npm run start",
    "start": "run-p start:**",
    "start:app": "npm run clean && gatsby develop",
    "start:lambda": "netlify-lambda serve functions-src",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "refresh": "rm -fr node_modules && npm install",
    "refresh-self": "rm -fr node_modules && rm -fr package-lock.json && npm install",
    "lint": "run-p lint:**",
    "lint:prettier": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:typescript": "tsc",
    "format": "npm run lint:prettier",
    "test": "npm run lint",
    "postinstall": "netlify-lambda install"
  },
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@material-ui/core": "^4.9.12",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.51",
    "@material-ui/styles": "^4.9.10",
    "@n8tb1t/use-scroll-position": "^1.0.43",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/client": "^1.149.7",
    "algoliasearch": "^4.2.0",
    "axios": "^0.19.2",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.14.0",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "formik": "^2.1.4",
    "gatsby": "^2.20.12",
    "gatsby-background-image": "^1.1.1",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-algolia": "^0.11.0",
    "gatsby-plugin-feed": "^2.5.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-material-ui": "^2.1.8",
    "gatsby-plugin-netlify": "^2.3.10",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sharp": "^2.6.3",
    "gatsby-plugin-sitemap": "^2.4.2",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-source-sanity": "^5.0.6",
    "gatsby-transformer-sharp": "^2.5.2",
    "intersection-observer": "^0.10.0",
    "nodemailer": "^6.4.8",
    "query-string": "^6.12.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-headroom": "^3.0.0",
    "react-helmet": "^6.0.0",
    "react-instantsearch-dom": "^6.4.0",
    "react-scroll": "^1.7.16",
    "react-share": "^4.1.0",
    "react-visibility-sensor": "^5.1.1",
    "voca": "^1.4.0",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/deepmerge": "^2.2.0",
    "@types/loadable__component": "^5.10.0",
    "@types/node": "^13.11.1",
    "@types/nodemailer": "^6.4.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-headroom": "^2.2.1",
    "@types/react-helmet": "^5.0.15",
    "@types/react-instantsearch-dom": "^6.3.0",
    "@types/react-scroll": "^1.5.5",
    "@types/react-share": "^3.0.3",
    "@types/react-visibility-sensor": "^5.0.1",
    "@types/voca": "^1.4.0",
    "@types/yup": "^0.28.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "http-proxy-middleware": "0.20.0",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">= 12"
  }
}
